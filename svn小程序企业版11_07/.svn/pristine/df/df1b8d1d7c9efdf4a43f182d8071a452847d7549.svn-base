/* pages/wenzhen/index.wxss */

page {
    background: #f6f6f6;
}

.wenzhen_list {
    width: 100%;
    box-sizing: border-box;
    padding: 30rpx 40rpx;
}

.wenzhen_li {
    display:block;
    width: 320rpx;
    height: 155rpx;
    background: #fff;
    border-radius: 8rpx;
    float: left;
    margin-bottom: 30rpx;
    font-size: 28rpx;
    color: #141414;
    white-space: pre-wrap;
}
.wenzhen_li .text{
    width: 180rpx;
    margin-top:36rpx;
    float: left;
    text-align: center;

}
.wenzhen_li:nth-child(odd) {
    margin-right: 30rpx;
}

.wenzhen_li image {
    width: 96rpx;
    height: 96rpx;
    vertical-align: middle;
    margin:30rpx 15rpx 0 15rpx;
    float: left;
}
.wenzhen_li {
  font-size: 33rpx;
}

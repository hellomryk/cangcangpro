/* pages/uploadimg/uploadimg.wxss */
.uploadimg {
  width:100%;
  height:1400rpx;
  background:#020b1c;
}
.uploadimg_up_one_num {
  width:50rpx;
  height:50rpx;
  background:#12b564;
  color:#fff;
  font-size: 32rpx;
  line-height:50rpx;
  text-align: center;
  border-radius:50% 50%;
  margin-left: 30rpx;
}
.uploadimg_up_one_up {
  width: 115rpx;
  text-align: center;
  color:#14c86c;
  font-size: 28rpx;
  line-height:65rpx;
}
.uploadimg_up_dian {
  display: flex;
  align-items:flext-start;
  justify-content: center;
  margin-top: 25rpx;
}
.uploadimg_up_dian>view{
  width:6rpx;
  height:6rpx;
  border-radius:50% 50%;
  background:#12b564;
  margin-left:5rpx;
}
.uploadimg_up {
  display: flex;
  align-items:flex-start;
  justify-content: center;
}
.uploadimg_up_one_up_active {
  color:#81858e;
}
.uploadimg_up_one_num_active {
  color:#020b1c;
  background:#81858e;
}
.uploadimg_up_dian_active>view{
  background:#81858e;
}
.uploadimg_takephoto_text {
  color:#fff;
  font-size: 32rpx;
  line-height:52rpx;
  width:100%;
  text-align: center; 
  margin-top: 50rpx;
}
.uploadimg_takephoto_cont {
  color:#747983;
  font-size: 24rpx;
  line-height:52rpx;
  width:100%;
  text-align: center; 
}
.uploadimg_cen {
  width: 486rpx;
  height:486rpx;
  margin:0 auto;
  position: relative;
}
.uploadimg_cen_kuang {
  display: block;
  width: 486rpx;
  height:486rpx;
  position: absolute;
  left:0;
  top:0;
}
.uploadimg_cen_people {
  width:277rpx;
  height:425rpx;
  top:30rpx;
  left:50%;
  position: absolute;
  margin-left:-138rpx;
  z-index: 10;
}

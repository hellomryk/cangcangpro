page {
  background:#fff;
}
.y-talk-box-content-left-top {
  border-bottom:none;
  padding-bottom: 60rpx;
}
.mxjb_text_writebao {
  width:100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.baocun {
    width:46%;
    height:200rpx;
    background:#ddd;
    line-height:115rpx;
    text-align:center;
    font-size:28rpx;
    color:#141414;
    border-radius:49rpx;
}
.nobaocun {
    width:46%;
    height:200rpx;
    background:#ddd;
    line-height:115rpx;
    text-align:center;
    font-size:28rpx;
    color:#141414;
    border-radius:49rpx;
}
.kaishiceping_none {
  display: none;
} 
.kaishiceping_block {
  display: block;
}


.zhuzheng_head {
width:100%;
padding:40rpx;
box-sizing:border-box;
text-align:left;
font-size:32rpx;
padding-bottom:0;
color:#141414;


}
.group {
max-height:360rpx;
overflow-y:auto;
-webkit-overflow-scrolling:touch;
overflow-scrolling:touch;
margin-bottom:25rpx;

}
.btn_wzh {
width:35%;
height:60rpx;
line-height:60rpx;
font-size:32rpx;
background:#15c86c;
color:#fff;

}
.service_selection {
width:100%;
padding:40rpx;
box-sizing:border-box;

}
.service_selection .item {
float:left;
margin:0 20rpx 20rpx 0;

}
.checkbox {
float:left;
border:1rpx solid #ccc;
border-radius:8rpx;
height:65rpx;
padding:10rpx 20rpx;
font-size:24rpx;
box-sizing:border-box;
color:#999;

}
.service_selection .back {
    background: #15c86c;
    color: #fff;
    border: 1rpx solid #15c86c;
}


.mxjb_text_writebao>view{
  display: block;
  width:100%;
  height:50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size:28rpx;
  color:#999;
}
.mxjb_text_writebao {
  position:fixed;
  left:0;

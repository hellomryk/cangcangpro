          <image src='{{avatarUrl==""?"../../pics/wodeyonghu.png":avatarUrl}}' class='wodeyonghu'></image>
          <view class='mine_top_name_text'>
              <view class='mtnt_name'>{{nickName==''?nameText:nickName}}</view>
              <view class='mtnt_tui' bindtap='loginOrOut'>{{nickName==''?buttonText:buttonText1}}</view>
          </view>
        </view>
    </view>
  </view>


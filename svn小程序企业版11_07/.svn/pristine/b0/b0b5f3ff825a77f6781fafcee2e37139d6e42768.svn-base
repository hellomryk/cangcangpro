    data: {
        username:"主人",
        list:null,
        list1: ["鼻与咽部系统疾病", "肥胖与糖尿病疾病", "肺与肺部系统疾病", "肝脏系统疾病", "颈腰椎与关节疾病", "帕金森与糖尿病疾病", "皮肤系统疾病", "气管与支气管疾病", "上呼吸道系统疾病", "睡眠与抑郁疾病", "胃与胃部系统疾病", "心脏系统疾病", "血液与血压疾病", "其他",]

    },
    //跳转到症状列表
    gotozhengzhuang(e) {
        console.log(11111111)
        wx.setStorageSync('xitong',e.currentTarget.id); 
        console.log(e)
        // if (e.currentTarget.id =="1"){
            //存储userInfo
            wx.navigateTo({
                url: '/pages/xinzheng/index?sex=' + "" + '&age=' + "" + '&username=' + _this.data.username + '&openId=' + app.globalData.openId + '&name=' + e.currentTarget.id,
            })
        // }else{
            // wx.showToast({
            //     title: "暂未开放！",
            //     icon: 'none',
            //     duration: 600
            // })
        // }

    },
    /**
     * 生命周期函数--监听页面加载
     */
    onLoad: function (options) {
              _this=this;
        wx.showModal({
            title: '提示',
            content: '欢迎您进入疾病自诊系统。我们的疾病自诊有九大系统，涉及数千种疾病，您可以进入相关具体系统后，选择和组合是否伴随或者出现过的症状，我们可以迅速为您诊断出可能的疾病以及诊断报告。',
            success(res) {
                if (res.confirm) {
                    console.log('用户点击确定')
                } else if (res.cancel) {
                    console.log('用户点击取消')
                }
            }
        })
        //app.globalData.openId
        //获取登陆用户名
        wx.getSetting({
            success: function (res) {
                if (res.authSetting['scope.userInfo']) {
                    // 已经授权，可以直接调用 getUserInfo 获取头像昵称
                    wx.getUserInfo({
                        success: function (res) {
                            console.log(res.userInfo.nickName)
                            _this.setData({
                                username: res.userInfo.nickName
                            })

                        }
                    })
                }
            }
        })


//请求列表
        wx.request({
            url: 'https://chronic-api-part.infobigdata.com/api/disease/selfTesting/queryDiseaseSysDmension',
            data: {
            
            },
            header: {
                'content-type': 'application/x-www-form-urlencoded' // 默认值application/json
            },
            method: "POST",
            success: function (res) {
                console.log(res.data.data.list)
                _this.setData({ list: res.data.data.list})

            }
        })

      //11-10统计接口自诊自测
      wx.request({
        url: url + '/api/disease/selfTesting/statistics',
        data: {
          openId: app.globalData.openId,
          source: 1,
          userName: app.globalData.userName,
        },
        method: "POST",
        header: {
          "Content-Type": "application/x-www-form-urlencoded"
        },
        success(res) {
          console.log("11-10tongji")
          console.log(res)
        }
      })

    },

    /**

.uploadimg_cen_line {
  width:536rpx;
  height:57rpx;
  top:10rpx;
  left:-30rpx;
  position: absolute;
  z-index: 12;
  animation:photoline 8s linear 1s infinite;
}
@keyframes photoline {
  0% {top:0rpx;}
  25% {top:220rpx;}
  50% {top:440rpx;}
  75% {top:220rpx;}
  100% {top:0rpx;}
}

.uploadimg_cen_lineli {
    width:536rpx;
  height:57rpx;
  top:10rpx;
  left:-60rpx;
  position: absolute;
  animation:photolineline 8s linear 1s infinite;
}
@keyframes photolineline {
  0% {top:-35rpx;}
  25% {top:185rpx;}
  50% {top:405rpx;}
  75% {top:185rpx;}
  100% {top:-35rpx;}
}
.uploadimg_takephoto_bot {
  color:#fff;

/* pages/sudi_ps/index.wxss */
@import"../../wxParse/wxParse.wxss";
.box_ps{
    width: 100%;
    height: 100%;
    padding: 40rpx 30rpx;
    box-sizing: border-box;
}
.head{
    font-size: 40rpx;
    color: #141414;
}
.head_ps{
    font-size: 24rpx;
    line-height: 100rpx;
}
.head_ps text:nth-child(1){
    color: #14c86c;
    margin-right: 30rpx;
}
.head_ps text:nth-child(2){
    color: #999;
}
.cont{
    font-size: 32rpx;
    color: #141414;
    /* line-height: 70rpx; */
    text-indent: 50rpx;
    border-bottom: 1rpx solid #ddd;
    padding-bottom:50rpx; 
}
.xgss{
    font-size: 28rpx;
    color: #666;
    line-height: 80rpx;
}
.xgss_ps{
    font-size: 28rpx;
    color: #141414;
    line-height: 80rpx;
}
.xgss_ps text{

}
.block{
display: inline-block;
width: 45%;
overflow:hidden; 
text-overflow:ellipsis;
white-space:nowrap; 
}
.block:nth-child(odd){
margin-right: 8%;
}
.btn{
    width: 180rpx;
    height: 60rpx;
    line-height: 60rpx;   
    border-radius: 100rpx;
    float: left;
}
.btn:nth-child(1){
margin-right: 75rpx;
}
.btn image{
    width: 23rpx;
    height: 24rpx;
    vertical-align: middle;
    margin-left: 40rpx;
    margin-right: 15rpx;
}
.true{
    color: #14c86c;
    border: 1rpx solid #14c86c;
}
.false{
    color: #999;
    border: 1rpx solid #999;
}
.btn_box{
    width:450rpx;
    margin: 45rpx auto;
    font-size: 24rpx;
}
/* pages/wodexinxi/index.wxss */
page {
    background: #f6f6f6;
}

.head {
    width: 100%;
    background: #fff;
    /* padding: 40rpx 0; */
}
.head_list {
    width: 100%;
    height: 85rpx;
    border-bottom: 1rpx solid #eee;
    font-size: 28rpx;
    line-height: 85rpx;
    padding-left: 30rpx;
}

.head_list input {
    float: left;
    line-height: 85rpx;
    height: 85rpx;
    text-indent: 45rpx;
}

.head_list text {
    float: left;
    line-height: 85rpx;
    height: 85rpx;
}

.row-wrap {
    width: 100%;
    height: 85rpx;
    line-height: 85rpx;
    font-size: 28rpx;
    background: #fff;
    float: left;
    border-bottom: 1px solid #eee;
    padding-left: 25rpx;
}

.row-wrap .label {
    float: left;
}

.row-wrap picker {
    float: left;
    width: 85%;
}

.leave_text {
    float: left;
    text-align: left;
    width: 85%;
    color: #808080;
    padding-left: 45rpx;
}

.leave_warning {
    color: red;
    padding: 5rpx;
    float: left;
}

.leave_img {
    width: 26rpx;
    height: 26rpx;
    float: left;
    margin-top: 35rpx;
    margin-left: 10rpx;
}
.baocun{
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    font-size: 28rpx;
    color: #15c86c;
    text-align: center;
    margin-top: 20rpx;
    background: #fff;
}

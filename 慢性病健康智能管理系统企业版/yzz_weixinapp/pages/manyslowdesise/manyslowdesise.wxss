/* pages/manyslowdesise/manyslowdesise.wxss */
@import '/pages/index/index.wxss';
.food_list_right_money {
  max-width:400rpx;
  display: -webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
}
.food_list_right_title {
  padding-top:10rpx;
  color:#141414;
  font-size:32rpx;
}
.food_list {
  padding-bottom: 20rpx;
  border-bottom:2rpx #ddd dashed;
}
page {
  overflow-y: visible!important;
}
.center {
  text-align: center;
  line-height: 60rpx;
}
.food_list_search {
  width:100%;
  height:106rpx;
  background:#fff;
  display:flex;
  align-items:center;
  justify-content:center;
}
.food_list_search_box {
  width:670rpx;
  height:63rpx;
  background: #eeeeee;
  display: flex;
  align-items: center;
  justify-content:flex-start;
  border-radius:5rpx;
}
.flsb_img {
  width:24rpx;
  height:24rpx;
  margin-left: 20rpx;
}
.flsb_input {
  width:600rpx;
  margin-left: 20rpx;
  font-size: 26rpx;
  color: #999;
}
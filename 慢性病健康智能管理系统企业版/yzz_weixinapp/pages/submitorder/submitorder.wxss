/* pages/submitorder/submitorder.wxss */
page {
   background:#f6f6f6;
}
.tianjia{
    width: 100%;
    height: 100rpx;
    background: #fff;
    color: #15c86c;
    font-size: 32rpx;
    line-height: 100rpx;
    margin-bottom: 20rpx;
}
.tianjia view{
    width:35%;
    margin: 0 auto;

}
.tianjia image{
    width:40rpx;
    height:40rpx;
    vertical-align: middle;
}
.clearfix::after {
  content:'';
  clear: both;
  display: block;
}
.submitorder_top_leftimg {
  float:left;
  width:28rpx;
  height:36rpx;
  margin-top:50rpx;
  margin-left:30rpx;
}
.submitorder_top_rightimg {
  float:right;
  width:16rpx;
  height:36rpx;
  margin-top:54rpx;
  margin-right:30rpx;
}
.submitorder_top_cen {
  /* float:left; */
  width:646rpx;
  padding-top:20rpx;
  margin-left:78rpx;
}
.submitorder_top_cen_title {
  font-size:32rpx;
  line-height: 53rpx;
  color:#141414;
}
.submitorder_top_cen_address {
  font-size:24rpx;
  line-height: 53rpx;
  color:#999999;
}
.submitorder_top {
  width:100%;
  height:137rpx;
  background:#ffffff;
  border-bottom: 1rpx dashed #1eda7a;
}
.submitorder_zong {
  background:#f6f6f6;
  height:1090rpx;
  overflow-y:scroll;
}
.submitorder_cen {
  background:#fff;
}
.submitorder_cen_title {
  font-size:24rpx;
  line-height: 82rpx;
  color:#1eda7a;
  padding-left:32rpx;
  border-bottom: 1px solid #e6e6e6;
}
.submitorder_cen_list {
  max-height:471rpx;
  overflow-y:scroll;
}
.submitorder_cen_listli_imgleft {
  float: left;
  width:168rpx;
  height:131rpx;
  margin-top: 34rpx;
}
.submitorder_cen_listli_right {
  float:right;
  margin:144rpx 46rpx 0 0;
}
.sclc_title {
  font-size:32rpx;
  line-height: 50rpx;
  color:#141414;
}
.sclc_standard {
  font-size:24rpx;
  line-height: 50rpx;
  color:#999999;
}
.sclc_money {
  font-size:24rpx;
  line-height: 60rpx;
  color:#141414;
}
.submitorder_cen_listli_center {
  margin-left:221rpx;
  margin-top:34rpx;
  height:190rpx;
}
.submitorder_cen_listli {
  height:190rpx;
  border-top: 1rpx solid #e6e6e6;
  margin-left:36rpx;
}
.submitorder_cen_listli_right {
    font-size:24rpx;
  color:#999999;
}
.submitorder_cen_listli:first-child {
  border-top: none;
}
.submitorder_money_list_good {
    float:left;
    font-size:24rpx;
    line-height: 67rpx;
    color:#141414;
    margin-left:30rpx;
}
.submitorder_money_list_number {
    float:right;
    font-size:24rpx;
    line-height:67rpx;
    color:#999999;
     margin-right:30rpx;
}
.submitorder_money {
  background:#fff;
  margin-top:20rpx;
}
.border-top {
  border-top: 1rpx solid #e6e6e6;
}
.border-top>.submitorder_money_list_good,.border-top>.submitorder_money_list_number {
  line-height:85rpx;
}
border-top>.submitorder_money_list_number {
  color:#fd432a;
}
.submitorder_remark {
  background:#fff;
  margin-top:20rpx;
  height:88rpx;
}
.submitorder_remark_text {
    float:left;
    font-size:24rpx;
    line-height: 88rpx;
    color:#141414;
    margin-left:30rpx;
}
.submitorder_remark_input {
    float:left;
    font-size:24rpx;
    line-height:88rpx;
    color:#141414;
    margin-left:30rpx;
    margin-top:18rpx;
}
.submitorder_remark_input::-webkit-input-placeholder{
    line-height:88rpx;
    color:#999999;
}
.submitorder_pay {
  position:fixed;
  bottom:0;
  left:0;
  width:100%;
  height:101rpx;
  border-top:1rpx solid #e6e6e6;
  background: #fff;
}
.submitorder_pay_confirm {
    float:right;
    font-size:32rpx;
    line-height: 100rpx;
    color:#ffffff;
    width:250rpx;
    height:100rpx;
    text-align: center;
    background:-webkit-linear-gradient(left,#22e380,#14c86c);
}
.submitorder_pay_money {
    float:right;
    font-size:32rpx;
    line-height: 100rpx;
    color:#141414;
    margin-right:34rpx;
}
.submitorder_pay_money >text {
    color:#fd432a;
}